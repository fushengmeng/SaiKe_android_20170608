<?xml version="1.0" encoding="utf-8"?>
<com.keruiyun.saike.uiview.SquareLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="@dimen/margin"
        android:layout_marginBottom="@dimen/margin"
        >
        <com.bilibili.magicasakura.widgets.TintImageView
            android:id="@+id/img_control"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/sk_index_29"
            android:padding="@dimen/margin"
            android:background="@color/img_no_select"/>
        <TextView
            android:id="@+id/txt_control"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/index_control"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            android:layout_marginTop="@dimen/margin"
            android:layout_marginBottom="@dimen/margin_l"/>


</com.keruiyun.saike.uiview.SquareLinearLayout>
