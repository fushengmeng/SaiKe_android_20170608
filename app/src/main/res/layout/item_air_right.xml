<?xml version="1.0" encoding="utf-8"?>
<com.keruiyun.saike.uiview.SquareLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center"
    android:background="@drawable/bg_index_air">
    <com.bilibili.magicasakura.widgets.TintImageView
        android:id="@+id/img_air_runparams"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin"
        android:layout_marginLeft="@dimen/margin_l"
        android:layout_marginRight="@dimen/margin_l"
        android:src="@drawable/sk_index_29"/>
    <TextView
        android:id="@+id/txt_air_runparams"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/index_control"
        android:textSize="18px"
        android:layout_marginLeft="@dimen/margin_l"
        android:layout_marginRight="@dimen/margin_l"
        android:layout_marginBottom="@dimen/margin"/>

</com.keruiyun.saike.uiview.SquareLinearLayout>
